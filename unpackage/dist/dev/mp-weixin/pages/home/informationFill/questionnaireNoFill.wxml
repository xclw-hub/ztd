<view><view class="nav"><view class="head"><view id="left" data-event-opts="{{[['tap',[['clickBack',['$event']]]]]}}" bindtap="__e"><image id="left" src="../../../static/enterprise/return.png"></image></view></view><block wx:if="{{true}}"><view class="tips"><text>{{"填报开始时间："+questionnaire.startTime}}</text><text>{{"填报结束时间："+questionnaire.endTime}}</text></view></block></view><view class="body"><view class="description"><text class="description-title">{{questionnaire.title}}</text><view class="description-body"><text>{{"发布人："+questionnaire.publisher}}</text><text>{{"审核人："+questionnaire.checker}}</text><text>{{"时间："+questionnaire.time}}</text></view><view class="description-content"><text>{{questionnaire.discription}}</text></view></view><view class="question"><block wx:for="{{questionnaire.content}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="question-item"><block wx:if="{{item.kind===0}}"><view class="single-choice"><text class="choice-question">{{index+1+"."+item.question}}</text><radio-group data-event-opts="{{[['change',[['radioChange',['$event',index]]]]]}}" class="single-choice-group" bindchange="__e"><block wx:for="{{item.answer}}" wx:for-item="answerItem" wx:for-index="anserIndex" wx:key="anserIndex"><view class="single-choice-group-item"><view><radio id="{{answerItem.value}}" value="{{answerItem.value}}" checked="{{answerItem.checked}}"></radio></view><label class="single-choice-group-item-value" for="{{answerItem.value}}"><text>{{answerItem.value}}</text></label></view></block></radio-group></view></block><block wx:else><block wx:if="{{item.kind===1}}"><view class="multiple-choice"><text class="choice-question">{{index+1+"."+item.question}}</text><checkbox-group data-event-opts="{{[['change',[['checkboxChange',['$event',index]]]]]}}" class="multiple-choice-group" bindchange="__e"><block wx:for="{{item.answer}}" wx:for-item="answerItem" wx:for-index="anserIndex" wx:key="anserIndex"><label class="multiple-choice-group-item"><view><checkbox value="{{answerItem.value}}" checked="{{answerItem.checked}}"></checkbox></view><view class="multiple-choice-group-item-value"><text>{{answerItem.value}}</text></view></label></block></checkbox-group></view></block><block wx:else><view class="essay"><text class="choice-question">{{index+1+"."+item.question}}</text><textarea data-event-opts="{{[['blur',[['bindTextAreaBlur',['$event',index]]]]]}}" class="essay-input" value="{{item.result}}" bindblur="__e"></textarea></view></block></block></view></block><button type="default" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e"><text>提交</text></button></view></view></view>